{
  "name": "angular-io-example",
  "version": "1.0.0",
  "private": true,
  "description": "Example project from an angular.io guide.",
  "scripts": {
    "lint": "tslint ./src/**/*.ts -t verbose",
    "serve": "ng serve",
    "start": "node server.js",
    "test": "ng test",
    "test:parallel": "ng test --karma-config src/karma-parallel.conf.js --watch false --progress false --browsers ChromeHeadless",
    "test:headless": "ng test --watch false --progress false --browsers ChromeHeadless",
    "build": "ng build --prod",
    "e2e": "ng e2e",
    "ng": "ng",
    "precommit-check": "echo \"Starting Precommit\" && npm run lint && exit 0",
    "postinstall": "ng build --aot --prod"
  },
  "pre-commit": [
    "precommit-check"
  ],
  "engines": {
    "node": "10.9.0",
    "npm": "6.4.0"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^6.1.4",
    "@angular/common": "^6.1.4",
    "@angular/compiler": "^6.1.4",
    "@angular/core": "^6.1.4",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.1.4",
    "@angular/platform-browser-dynamic": "^6.1.4",
    "@angular/router": "^6.0.0",
    "@angular/upgrade": "^6.0.0",
    "angular-in-memory-web-api": "^0.6.1",
    "codelyzer": "^4.4.4",
    "core-js": "^2.5.4",
    "express": "^4.16.3",
    "path": "^0.12.7",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.24"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.1",
    "@angular/cli": "^6.2.1",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/platform-server": "^6.0.0",
    "@types/jasmine": "~2.8.0",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^10.9.4",
    "enhanced-resolve": "^4.1.0",
    "jasmine-core": "~3.2.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^1.3.1",
    "karma-parallel": "^0.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "lodash": "^4.17.11",
    "phantomjs-prebuilt": "^2.1.7",
    "pre-commit": "^1.2.2",
    "protractor": "^5.4.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.9.1",
    "typescript": "2.9.2"
  },
  "repository": {}
